{
  "metadata": [
    {
      "src": [
        {
          "src": "../biocs",
          "files": [ "core/core.csproj" ]
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "{api,articles}/**/*.{md,yml}",
          "index.md",
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "overwrite": "apidoc/**/*.md",
    "dest": "../docs",
    "globalMetadataFiles": [ "global.json" ],
    "template": [ "default", "modern" ],
    "maxParallelism": 4,
    "xref": [ "https://learn.microsoft.com/en-us/dotnet/.xrefmap.json" ]
  }
}